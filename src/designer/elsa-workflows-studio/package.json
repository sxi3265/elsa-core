{
  "name": "@elsa-workflows/elsa-workflows-studio",
  "version": "2.0.1",
  "description": "Elsa Workflow Studio is a collection of web components to manage Elsa workflows stored on an Elsa Workflows server.",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/elsa-workflows-studio/elsa-workflows-studio.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "push": "npm publish --access public",
    "push:preview": "npm publish --access public --tag preview",
    "build": "npm run build:tailwind:prod & stencil build",
    "build:dev": "stencil build --dev --tailwind:dev",
    "start:dev": "stencil build --dev --tailwind:dev --watch --serve",
    "start": "stencil build --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "build:tailwind:dev": "cross-env NODE_ENV=development tailwind build ./tailwind.css -o ./src/globals/tailwind.css & copyfiles -u 2 \"./src/globals/tailwind.css\" \"./src/assets/styles\"",
    "build:tailwind:prod": "cross-env NODE_ENV=production tailwind build ./tailwind.css -o ./src/globals/tailwind.css & copyfiles -u 2 \"./src/globals/tailwind.css\" \"./src/assets/styles\""
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@stencil/core": "^2.5.2",
    "@stencil/postcss": "^2.0.0",
    "@stencil/router": "^1.0.1",
    "@stencil/state-tunnel": "^1.0.1",
    "@stencil/store": "^1.4.1",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/jit": "^0.1.18",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "10.2.5",
    "axios": "^0.21.1",
    "cssnano": "^4.1.10",
    "d3": "^6.6.2",
    "dagre-d3": "^0.6.4",
    "el-transition": "^0.0.7",
    "eslint": "7.23.0",
    "js-event-bus": "^1.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "monaco-editor": "^0.23.0",
    "postcss": "^8.2.9",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "stencil-click-outside": "^1.7.1",
    "tailwindcss": "2.1.1",
    "tslint": "6.1.3",
    "tslint-stencil": "1.0.1",
    "typescript": "4.2.3",
    "typescript-tslint-plugin": "1.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/d3": "^6.3.0",
    "@types/dagre-d3": "^0.6.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3"
  },
  "license": "MIT"
}
